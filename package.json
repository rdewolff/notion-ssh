{
  "name": "notion-ssh",
  "version": "0.1.0",
  "description": "SSH virtual manager for Notion pages as Markdown files",
  "main": "dist/src/server.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/src/server.js",
    "check": "npm run lint && npm run test && npm run build",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdewolff/notion-ssh.git"
  },
  "keywords": [
    "notion",
    "ssh",
    "markdown"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdewolff/notion-ssh/issues"
  },
  "homepage": "https://github.com/rdewolff/notion-ssh#readme",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@notionhq/client": "^5.1.0",
    "dotenv": "^17.2.3",
    "pino": "^10.0.0",
    "slugify": "^1.6.6",
    "ssh2": "^1.17.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/ssh2": "^1.15.5",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0"
  }
}
